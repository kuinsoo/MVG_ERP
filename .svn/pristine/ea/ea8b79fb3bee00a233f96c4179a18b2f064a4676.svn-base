package humanResources.vo;

import javafx.scene.control.CheckBox;

public class EmployeeVo {
  private CheckBox emp_chk;
  private String emp_code;
  private String emp_pos;
  private String emp_dept;
  private String emp_pass;
  private String emp_name;
  private String emp_hp;
  private String emp_regno1;
  private String emp_regno2;
  private String emp_addr;
  private String emp_gender;
  private String emp_hdate;
  private String emp_remp;
  private String emp_note;
  private String emp_salt;

  public EmployeeVo() {
    super();
  }

  public EmployeeVo(String emp_code, String emp_pos, String emp_dept, String emp_pass, String emp_name, String emp_hp,
                    String emp_regno1, String emp_regno2, String emp_addr, String emp_gender, String emp_hdate, String emp_remp,
                    String emp_note, String emp_salt) {
    super();
   
    this.emp_code = emp_code;
    this.emp_pos = emp_pos;
    this.emp_dept = emp_dept;
    this.emp_pass = emp_pass;
    this.emp_name = emp_name;
    this.emp_hp = emp_hp;
    this.emp_regno1 = emp_regno1;
    this.emp_regno2 = emp_regno2;
    this.emp_addr = emp_addr;
    this.emp_gender = emp_gender;
    this.emp_hdate = emp_hdate;
    this.emp_remp = emp_remp;
    this.emp_note = emp_note;
    this.emp_salt = emp_salt;
  }
 
  public void setEmp_chk(CheckBox chk) {
	  this.emp_chk = chk;
  }
  public CheckBox getEmp_chk() {
	return emp_chk;	
  }
  public String getEmp_code() {
	return emp_code;
  }
  public void setEmp_code(String emp_code) {
    this.emp_code = emp_code;
  }
  public String getEmp_pos() {
    return emp_pos;
  }
  public void setEmp_pos(String emp_pos) {
    this.emp_pos = emp_pos;
  }
  public String getEmp_dept() {
    return emp_dept;
  }
  public void setEmp_dept(String emp_dept) {
    this.emp_dept = emp_dept;
  }
  public String getEmp_pass() {
    return emp_pass;
  }
  public void setEmp_pass(String emp_pass) {
    this.emp_pass = emp_pass;
  }
  public String getEmp_name() {
    return emp_name;
  }
  public void setEmp_name(String emp_name) {
    this.emp_name = emp_name;
  }
  public String getEmp_hp() {
    return emp_hp;
  }
  public void setEmp_hp(String emp_hp) {
    this.emp_hp = emp_hp;
  }
  public String getEmp_regno1() {
    return emp_regno1;
  }
  public void setEmp_regno1(String emp_regno1) {
    this.emp_regno1 = emp_regno1;
  }
  public String getEmp_regno2() {
    return emp_regno2;
  }
  public void setEmp_regno2(String emp_regno2) {
    this.emp_regno2 = emp_regno2;
  }
  public String getEmp_addr() {
    return emp_addr;
  }
  public void setEmp_addr(String emp_addr) {
    this.emp_addr = emp_addr;
  }
  public String getEmp_gender() {
    return emp_gender;
  }
  public void setEmp_gender(String emp_gender) {
    this.emp_gender = emp_gender;
  }
  public String getEmp_hdate() {
    return emp_hdate;
  }
  public void setEmp_hdate(String emp_hdate) {
    this.emp_hdate = emp_hdate;
  }
  public String getEmp_remp() {
    return emp_remp;
  }
  public void setEmp_remp(String emp_remp) {
    this.emp_remp = emp_remp;
  }
  public String getEmp_note() {
    return emp_note;
  }
  public void setEmp_note(String emp_note) {
    this.emp_note = emp_note;
  }

public String getEmp_salt() {
	return emp_salt;
}

public void setEmp_salt(String emp_salt) {
	this.emp_salt = emp_salt;
}

 
}
